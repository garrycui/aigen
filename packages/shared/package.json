{
  "name": "@aigen/shared",
  "version": "1.0.0",
  "private": true,
  "main": "src/index.ts",
  "types": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "build:server": "tsc -p tsconfig.server.json",
    "test": "vitest",
    "dev:server": "tsx watch src/server/server_dev.ts",
    "start:server": "tsx src/server/server.ts",
    "start:prod": "node dist/server/server.js",
    "populate": "node ../../scripts/populateMockData.js",
    "backup": "node ../../scripts/backup.js create",
    "backup:list": "node ../../scripts/backup.js list",
    "backup:restore": "node ../../scripts/backup.js restore",
    "generate-content": "tsx ../../scripts/generateContent.ts"
  },
  "dependencies": {
    "axios": "^1.8.3",
    "firebase": "^11.4.0",
    "openai": "^4.87.4",
    "stripe": "^17.7.0",
    "vader-sentiment": "^1.1.3",
    "express": "^4.21.2",
    "node-cron": "^3.0.3",
    "dotenv": "^16.4.7"
  },
  "devDependencies": {
    "typescript": "^5.8.2",
    "vitest": "^3.0.9",
    "@types/express": "^5.0.0",
    "@types/node-cron": "^3.0.11",
    "tsx": "^4.19.3"
  }
}