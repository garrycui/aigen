{
  "name": "@aigen/shared",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "build:server": "tsc -p tsconfig.server.json",
    "build:dev-server": "tsc -p tsconfig.server.json && echo \"Development server compiled\"",
    "test": "vitest",
    "dev:server": "npm run build:dev-server && nodemon dist/server/server_dev.js",
    "start:server": "tsx src/server/server.ts",
    "start:prod": "node dist/server/server.js",
    "populate": "node ../../scripts/populateMockData.js",
    "backup": "node ../../scripts/backup.js create",
    "backup:list": "node ../../scripts/backup.js list",
    "backup:restore": "node ../../scripts/backup.js restore",
    "generate-content": "tsx ../../scripts/generateContent.ts"
  },
  "dependencies": {
    "axios": "^1.8.4",
    "firebase": "^11.5.0",
    "openai": "^4.89.0",
    "stripe": "^17.7.0",
    "vader-sentiment": "^1.1.3",
    "express": "^4.21.2",
    "node-cron": "^3.0.3",
    "dotenv": "^16.4.7",
    "react-intersection-observer": "^9.16.0",
    "@uiw/react-codemirror": "^4.23.10",
    "@codemirror/lang-css": "^6.3.1",
    "@codemirror/lang-html": "^6.4.9",
    "@codemirror/lang-javascript": "^6.2.3",
    "@codemirror/lang-python": "^6.1.7",
    "@codemirror/lang-sql": "^6.8.0",
    "@codemirror/theme-one-dark": "^6.1.2",
    "@uiw/codemirror-theme-github": "^4.23.10",
    "@uiw/codemirror-theme-vscode": "^4.23.10"
  },
  "devDependencies": {
    "typescript": "^5.8.2",
    "vitest": "^3.0.9",
    "@types/express": "^5.0.1",
    "@types/node-cron": "^3.0.11",
    "tsx": "^4.19.3",
    "nodemon": "^3.1.9"
  }
}